resolution 480
velocity 127
gatetime_cut_off 10

time_signature 4/4
tempo 120.0


pattern define A1 {
  channel 9
  step 120

    C1   C#2

    C1   F#

       E F# velocity=100

    C    F#

       E

    C    F#

       E
    C    F#
       E F#
         A#
  }
}


sequence {
  title 'syntax example'

  pattern A1 from=1:1:0

  channel 9
  bank_select msb=127 lsb=0
  program_change 0
  note step=120 {
    C1   C#2
    C1   F#
       E F# velocity=100
    C    F#
       E
    C    F#
       E
    C    F#
       E F#
         A#
  }
}


set default velocity=127
set default gatetime=240

track "syntax sample"

time_signature 4/4
tempo 120.0

set default step=120
set channel=9

bank_select step=0 msb=127 lsb=0
program_change step=0 0

note C1,     C#2
  rest
note C1,     F#
  rest
note     E,  F#
  rest
note C,      F#
note     E
note C,      F#
note     E
note C,      F#
  rest
note     E,  F#
  rest
note         A#
  rest

set channel=1

bank_select step=0 msb=0 lsb=0
program_change step=0 2

note C2,E,G
  tie
  tie
  tie
note D
  tie
note E
note F
note G
  tie
note A
  tie
note B
  tie
note C3,E,G,C
  rest

track_end


pattern a1
pattern_end

pattern a2 extends a1 mix from 4:0:0 {
}

a1 replace from 4:0:0 {
  note << EOC
  C E G
  EOC
}
